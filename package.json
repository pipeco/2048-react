{
  "name": "2048-react-ES2015",
  "version": "0.2.0",
  "description": "A 2048 clone written with React, SASS and ES2015",
  "repository": {
    "type": "git",
    "url": "https://github.com/rubenjgarcia/2048-react"
  },
  "authors": [
    "Ruben J Garcia <rubenjgarciab@gmail.com>",
    "Ivan Vergiliev <https://github.com/IvanVergiliev>"
  ],
  "scripts": {
    "start": "npm run build && ./node_modules/.bin/static -a 0.0.0.0 -p $VCAP_APP_PORT",
    "dev": "./node_modules/.bin/webpack && npm run server",
    "build": "npm install && ./node_modules/.bin/webpack",
    "server": "./node_modules/.bin/webpack-dev-server --hot"
  },
  "dependencies": {
    "babel-core": "^6.1.4",
    "babel-loader": "^6.0.1",
    "babel-plugin-react-transform": "^1.1.1",
    "babel-preset-es2015": "^6.1.4",
    "babel-preset-react": "^6.1.4",
    "babel-preset-stage-0": "^6.1.4",
    "classnames": "^2.2.3",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "node-sass": "^3.4.2",
    "node-static": "^0.7.7",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.9.10",
    "webpack-dev-server": "*"
  },
  "license": "MIT",
  "engines": {
    "node": ">5",
    "npm": ">3.3"
  }
}
